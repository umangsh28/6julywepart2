<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2">

    <TextView
        android:id="@+id/tnamew"
        android:layout_width="225dp"
        android:layout_height="60dp"
        android:layout_marginTop="60dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@id/tlastw"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.408"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tlastw"
        android:layout_width="209dp"
        android:layout_height="62dp"
        android:layout_marginTop="80dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@id/tmailw"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.455"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tnamew" />

    <TextView
        android:id="@+id/tmailw"
        android:layout_width="198dp"
        android:layout_height="75dp"
        android:layout_marginTop="72dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@id/tnumw"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.431"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tlastw" />

    <TextView
        android:id="@+id/tnumw"
        android:layout_width="209dp"
        android:layout_height="60dp"
        android:layout_marginTop="52dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tmailw" />

    <Button
        android:id="@+id/button"
        android:layout_width="144dp"
        android:layout_height="71dp"
        android:layout_marginTop="24dp"
        android:text="next"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.411"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tseat" />

    <EditText
        android:id="@+id/tseat"
        android:layout_width="107dp"
        android:layout_height="40dp"
        android:layout_marginTop="32dp"
        android:hint="No. of seat"
        app:layout_constraintBottom_toTopOf="@id/button"
        app:layout_constraintEnd_toStartOf="@+id/tdate"
        app:layout_constraintHorizontal_bias="0.474"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tnumw"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/tdate"
        android:layout_width="112dp"
        android:layout_height="43dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="76dp"
        android:hint="date booked"
        app:layout_constraintBottom_toTopOf="@id/button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/tseat"
        app:layout_constraintTop_toBottomOf="@id/tnumw"
        app:layout_constraintVertical_bias="0.285" />


</androidx.constraintlayout.widget.ConstraintLayout>